{
  "redirects": [
    { "source": "/", "destination": "https://github.com/benborgers/potion" }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "s-maxage=10"
        }
      ]
    }
  ]
}